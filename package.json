{
  "private": true,
  "scripts": {
    "build": "$npm_package_env_PROD next build",
    "dev": "$npm_package_env_DEV next dev -p 4100",
    "deploy": "rm -rf node_modules/.cache && yarn build && yarn export && touch out/.nojekyll && git add out/ && git commit -m \"Deploy Next.js to gh-pages\" && git subtree push --prefix out origin gh-pages",
    "export": "$npm_package_env_PROD next export",
    "start": "$npm_package_env_PROD next start"
  },
  "env": {
    "COMMON": "env-cmd -f ./.env/common.env.js",
    "DEV": "env-cmd -f ./.env/development.env.js",
    "PROD": "env-cmd -f ./.env/production.env.js",
    "TEST": "env-cmd -f ./.env/test.env.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.2",
    "@radix-ui/react-dialog": "^0.1.7",
    "env-cmd": "^10.1.0",
    "next": "12.0.9",
    "prop-types": "^15.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/eslint-parser": "7.17.0",
    "@babel/plugin-transform-react-jsx": "^7.17.3",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@next/eslint-plugin-next": "12.0.9",
    "eslint": "^8.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.0.9",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1"
  }
}
